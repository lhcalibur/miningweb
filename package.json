  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
